<launch>
  <!-- <arg name="map" default="6jiao1.yaml" /> -->
  <param name="/use_sim_time" value="false" />

  <node name="map_server" pkg="map_server" type="map_server" 
    args="/home/learningx/qingzhou_ws/src/qingzhou_nav/maps/carto.yaml"/>

  <arg name="load_state_filename" default="/home/learningx/qingzhou_ws/src/qingzhou_nav/maps/carto.pbstream"/>
  <!--开启cartographer定位-->
  <node name="cartographer_node" pkg="cartographer_ros"
		type="cartographer_node" args="
			-configuration_directory $(find qingzhou_nav)/config
			-configuration_basename robot_localization_2d.lua
			-load_state_filename $(arg load_state_filename)"
		output="screen">
    <remap from="imu" to="imu_data" />
    <remap from="odom" to="odom_data" />
    <remap from="scan" to="scan" />
  </node>

  <!-- <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros" 
    type="cartographer_occupancy_grid_node" args="
        -resolution 0.05" 
    output="screen">
  </node> -->



  <!--rviz-->
  <!-- <node name="MPC_track_world_rviz" pkg="rviz" type="rviz" required="true" args="-d $(find MPC_track)/rviz/track_world.rviz"/> -->

</launch>




